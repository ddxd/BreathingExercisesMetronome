<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <solid android:color="#0000"/>
            <stroke
                android:width="5dp"
                android:color="@color/white" />
            <corners
                android:radius="15dp" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#0000"/>
            <stroke
                android:width="5dp"
                android:color="@color/white" />
            <corners
                android:radius="15dp" />
        </shape>
    </item>


</selector>
